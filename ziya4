class player:
  def play(self):
     print("The player is playing cricket.")

class Batsman(player):
  def play(self):
     print("Yhe batsman is batting:")

class Bowler(player):
   def play(self):
      print("the bowler is bowling.")
# creating objects of the Batsman and Bowler classes batsman=batsman()
bowler=Bowler()

# calling the play()
for each object
batsman.play()
bowler.play()
